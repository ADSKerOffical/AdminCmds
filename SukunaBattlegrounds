local HOn = nil

local Contact = [[
Commands:
!Hell_(On/Off)       !punchaura_(On/Off)
!speed(Count)        !shelling1_(On/Off)
!jumppower(Count)    !shelling2_(On/Off)
!reset         !meteors_(On/Off)
!Blackhole (Power)     !meteorrain_(On/Off)
!Arrow         !blackhole(Count)
!Buff_(On/Off)      !atomic(Power)
!UltraPurple        !sneez_(On/Off)
!Laugh
!Mahoraga
!regen_(On/Off)
]]

local function onPlayerChatted(message, player)
    local lowercaseMessage = message:lower()
    
    if lowercaseMessage:find("!cmds") then
OrionLib:MakeNotification({
	Name = "Budgie Hub",
	Content = Contact,
	Image = "rbxassetid://4483345998",
	Time = 20
})
    end
end

game.Players.LocalPlayer.Chatted:Connect(onPlayerChatted)

local function onPlayerChatted(message, player)
    local lowercaseMessage = message:lower()
    
    local parts = lowercaseMessage:split(" ")
    if parts[1]:find("!speed") then
        local count = tonumber(parts[2])
        if count and count > 0 then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = count
        end
    end
end

game.Players.LocalPlayer.Chatted:Connect(onPlayerChatted)

