local MSon = nil
local function onPlayerChatted(message, player)
    local lowercaseMessage = message:lower()
    
    if lowercaseMessage:find("!meteors_on") then
       MSon = true
        while MSon do
    wait(0.6)
    local player = game.Players.LocalPlayer
    local mouse = player:GetMouse()

    local targetPosition = mouse.Hit.Position
    local args = {
        [1] = CFrame.new(targetPosition),
        [2] = math.huge
    }

    local character = player.Character or player.CharacterAdded:Wait()
    local supernova = character:FindFirstChild("Supernova")
    if supernova then
        local swordRain = supernova:FindFirstChild("Sword Rain")
        if swordRain then
            swordRain.RemoteEvent:FireServer(unpack(args))
        end
    end
end
    end
end

